namespace api

asset Crop identified by cropId {
  o String cropId
  o String name
  o String description
  o String quantity
  o String price
  o String location
  o Boolean availability  default = true
}
participant Seller identified by sellerId {
o String sellerId
o String firstName
o String lastName
o String contact
}
participant Buyer identified by buyerId {
o String buyerId
o String firstName
o String lastName
o String contact
}
transaction CropTransfer {
  --> Seller from
  --> Buyer to
}
